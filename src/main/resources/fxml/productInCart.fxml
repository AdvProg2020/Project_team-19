<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" prefHeight="200.0" prefWidth="380.0" style="-fx-border-color: #ff8080; -fx-border-width: 4;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <VBox alignment="CENTER" prefHeight="219.0" prefWidth="209.0" spacing="5.0">
         <children>
            <Label fx:id="productNameLabel" text="Label" />
            <Label fx:id="sellerNameLabel" text="Label" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label fx:id="priceLabel" text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="37.0" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label fx:id="discountPriceLabel" text="Price After Discount" />
                  <Label text="Label" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <ImageView fx:id="decrease" fitHeight="21.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/icons8-minus-30.png" />
                     </image>
                  </ImageView>
                  <Label fx:id="countLabel" text="Label" />
                  <ImageView fx:id="increase" fitHeight="22.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/download.png" />
                     </image>
                  </ImageView>
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="100.0" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Total Price" />
                  <Label fx:id="totalPriceLabel" prefHeight="27.0" prefWidth="63.0" text="Label" />
               </children>
            </HBox>
         </children>
      </VBox>
      <ImageView fx:id="productImage" fitHeight="136.0" fitWidth="153.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1">
         <image>
            <Image url="@../images/product.png" />
         </image></ImageView>
   </children>
</GridPane>
