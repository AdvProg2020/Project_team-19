<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlController.AllAuctionsMenu">
   <children>
      <ImageView fx:id="cart" fitHeight="50.0" fitWidth="50.0" layoutX="717.0" layoutY="14.0" onMouseClicked="#cartClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/cart.png" />
         </image>
      </ImageView>
      <ImageView fx:id="wallet" fitHeight="50.0" fitWidth="50.0" layoutX="650.0" layoutY="14.0" onMouseClicked="#walletClicked" opacity="0.8" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/wallet.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
