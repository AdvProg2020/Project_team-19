<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<HBox alignment="CENTER" prefHeight="50.0" prefWidth="126.0" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="categoryName" prefHeight="27.0" prefWidth="60.0" text="Label" />
      <ImageView fx:id="editIcon" fitHeight="25.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/pencil.png" />
         </image>
      </ImageView>
      <ImageView fx:id="removeIcon" fitHeight="25.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/remove.png" />
         </image>
      </ImageView>
   </children>
</HBox>
