<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlController.AddSupport">
    <children>
        <ScrollPane hbarPolicy="NEVER" prefHeight="300.0" prefWidth="600.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="600.0">
                    <children>
                        <Label alignment="CENTER" layoutX="-1.0" layoutY="53.0" prefHeight="26.0" prefWidth="600.0" text="Add Support">
                            <font>
                                <Font name="Ebrima" size="17.0" />
                            </font>
                        </Label>
                        <TextField fx:id="username" layoutX="56.0" layoutY="136.0" prefHeight="26.0" prefWidth="484.0" promptText="Username" stylesheets="@textField.css" />
                        <PasswordField fx:id="password" layoutX="56.0" layoutY="185.0" prefHeight="26.0" prefWidth="484.0" promptText="Password" stylesheets="@textField.css" />
                        <TextField fx:id="firstName" layoutX="56.0" layoutY="234.0" prefHeight="26.0" prefWidth="484.0" promptText="First Name" stylesheets="@textField.css" />
                        <TextField fx:id="lastName" layoutX="56.0" layoutY="283.0" prefHeight="26.0" prefWidth="484.0" promptText="Last Name" stylesheets="@textField.css" />
                        <TextField fx:id="email" layoutX="56.0" layoutY="332.0" prefHeight="26.0" prefWidth="484.0" promptText="Email" stylesheets="@textField.css" />
                        <TextField fx:id="phone" layoutX="56.0" layoutY="381.0" prefHeight="26.0" prefWidth="484.0" promptText="Phone" stylesheets="@textField.css" />
                        <Button fx:id="registerBtn" defaultButton="true" layoutX="314.0" layoutY="430.0" mnemonicParsing="false" onAction="#registerBtnAction" prefHeight="26.0" prefWidth="226.0" styleClass="btn" stylesheets="@button.css" text="Register" />
                        <Button fx:id="cancelBtn" cancelButton="true" layoutX="56.0" layoutY="430.0" mnemonicParsing="false" onAction="#cancelBtnAction" prefHeight="26.0" prefWidth="226.0" styleClass="btn" stylesheets="@button.css" text="Cancel" />
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
    </children>

</AnchorPane>
