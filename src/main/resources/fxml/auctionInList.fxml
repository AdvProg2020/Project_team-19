<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="252.0" prefWidth="200.0" style="-fx-border-color: #017b9c;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints maxHeight="158.0" minHeight="10.0" prefHeight="29.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="206.0" minHeight="10.0" prefHeight="127.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="79.0" minHeight="10.0" prefHeight="48.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="79.0" minHeight="10.0" prefHeight="49.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <ImageView fx:id="productPic" fitHeight="135.0" fitWidth="137.0" pickOnBounds="true" preserveRatio="true" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
         <image>
            <Image url="@../images/product.png" />
         </image>
      </ImageView>
      <Label fx:id="productName" alignment="CENTER" prefHeight="37.0" prefWidth="175.0" text="name" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
         <font>
            <Font name="Verdana" size="16.0" />
         </font>
      </Label>
      <Label fx:id="endTime" prefHeight="38.0" prefWidth="170.0" text="date" textFill="RED" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
         <font>
            <Font name="Verdana" size="14.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="3">
         <children>
            <ImageView fx:id="profilePic" fitHeight="33.0" fitWidth="43.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="sellerName" alignment="CENTER" prefHeight="44.0" prefWidth="119.0" text="sellerName">
               <font>
                  <Font name="Verdana" size="17.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</GridPane>
