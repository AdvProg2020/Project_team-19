        <ChoiceBox fx:id="field" layoutX="50.0" layoutY="51.0" prefHeight="26.0" prefWidth="282.0" />
        <TextField fx:id="text" layoutX="50.0" layoutY="123.0" prefHeight="26.0" prefWidth="282.0" promptText="Password" stylesheets="@textField.css" />
